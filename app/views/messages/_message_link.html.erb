  <% if haveRoom == true %>
    <%= link_to "チャットへ", room_path(@roomId) %>
  <% else %>
    <%= form_with model: room do |f| %>
      <%= fields_for entry do |e| %>
        <%= e.hidden_field :user_id, value: user.id %>
      <% end %>
      <%= f.submit "チャットを始める", class: "btn btn-link" %>
    <% end %>
  <% end %>